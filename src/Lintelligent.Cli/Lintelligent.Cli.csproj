<Project Sdk="Microsoft.NET.Sdk">


    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net10.0</TargetFramework>
        <Version>1.1.0</Version>
        <!-- Suppress NuGet security warnings from Buildalyzer transitive dependencies -->
        <!-- These are runtime-only dependencies not exposed in our API surface -->
        <NoWarn>$(NoWarn);NU1608;NU1902;NU1903;NU1904</NoWarn>
    </PropertyGroup>


    <ItemGroup>
        <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="10.0.1"/>
        <PackageReference Include="Buildalyzer" Version="8.0.0"/>
    </ItemGroup>


    <ItemGroup>
        <ProjectReference Include="..\Lintelligent.AnalyzerEngine\Lintelligent.AnalyzerEngine.csproj"/>
        <ProjectReference Include="..\Lintelligent.Reporting\Lintelligent.Reporting.csproj"/>
    </ItemGroup>


</Project>
